<div class="page">
	<div class="header">
		{{#with header}}
		<header class="header_content">
			<div class="header_block">
				<img src='/svg/apple.svg' alt="Apple" class="header_icon">
				<nav class="header_block">
					{{#each links}}
					<a href="{{url}}" class="header_link">{{text}}</a>
					{{/each}}
				</nav>
			</div>
			<div class="header_block">
				<img src="/svg/search.svg" alt="Search" class="header_icon">
				<img src="/svg/profile.svg" alt="Profile" class="header_icon">
			</div>
		</header>
		{{/with}}
	</div>
	<div class="ad">
		{{#with header}}
		<div class="ad_content">
			<p class="ad_text">{{ad.title}}</p>
			<div class="product_action">
				{{#with ad.link}}
				<a href="{{url}}" class="product_link">{{text}}</a>
				{{/with}}
			</div>
		</div>
		{{/with}}
	</div>


	{{#with iphone_pro}}
	<section class="product product--big product--iphone-pro product--dark product--top">
		<div class="product_content">
			<h1 class="product_title">{{title}}</h1>
			<p class="product_description">{{descriptions}}</p>
			<div class="product_action">
				{{#with link}}
				{{#each text}}
				<a href="{{../link}}" class="product_link">{{this}}</a>
				{{/each}}
				{{/with}}

			</div>
		</div>
	</section>
	{{/with}}

	{{#with iphone}}
